{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": [
      "vite/client"
    ],
    "paths": {
      "react": [
        "./src/preact.d.ts"
      ],
      "react-dom": [
        "./src/preact.d.ts"
      ],
      "react/jsx-runtime": [
        "./src/preact.d.ts"
      ]
    }
  },
  "include": [
    "src",
    "wailsjs"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
